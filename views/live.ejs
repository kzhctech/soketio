  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <link rel="stylesheet" href="/static/index.css">
  <link rel="stylesheet" href="/static/bootstrap.min.css">
  <title><%= Title %></title>
  <script src="https://cdn.socket.io/socket.io-3.0.0.js"></script>
  <script>
	  const socket = io('<%= URL %>');
  </script>
  <script src="/static/index.js"></script>
</head>
<body onload="scroll()">
<div class="bg-primary">
<div class="container text-white mb-2 py-2">
<div class="row">
<div class="col">
<%= Title %>
</div>
</div>
</div>
</div>
<div class="container">
<div class="my-2">
<h5 id="statusbar" class="text-danger mb-2">
<%= Status %>
</h5>
</div>
</div>
	<div class="container">
		<div class="gwrap">
			<div class="iteam">
				<div id="adv">
					<img src="	<% if(LBB){ %> <%= ad %>	 <% } %>" width="100%" alt="" srcset="">
					<h4 class="text-center">END OF OVER</h4>
				</div>
			 		<div class="ground">
						<div class="cercel">
	    <div class="wicket">
	      <div id="ball"></div>
	      <div class="filder" id="f1"></div>
	      <div class="filder" id="f2"></div>
	      <div class="filder" id="f3"></div>
	      <div class="filder" id="f4"></div>
	      <div class="filder" id="f5"></div>
	      <div class="filder" id="f6"></div>
	      <div class="filder" id="f7"></div>
	      <div class="filder" id="f8"></div>
	      <div class="filder" id="f9"></div>
	    </div>
	  </div>
	</div>
		
			</div>
		</div>


		<% if(BatName){ %>


			<div class="card-it">
				<div class="card bg-white mx-2 mt-3  mb-3">
					<div class="card-header">
						  <div class="row">
							<div class="col-md-4 col-sm-12">
								<div class="row">
									<div class="col-4"><span id="Teambat1name"><%= BatName %></span></div>
									<div class="col-8 text-right"><h4 id="bat1Score"><%= BatRun %><small class="text-muted ml-2"></small></h4></div>
								</div>
							</div>
							<div class="col-md-4 col-sm-12">
								<% if(LBB){ %>	<div class="w-100 bg-primary text-white py-1 px-3 text-center"><h5 id="lb">Dot Ball</h5></div> <% } %>
							</div>
							<div class="col-md-4 col-sm-12">
								<% if(BowlName){ %>
									<div class="row">
										<div class="col-4"><span id="Teambat2name"><%= BowlName %></span></div>
												<div class="col-8 text-right"><h4 id="bat2Score"><%= BowlRun %><small class="text-muted ml-2"></small></h4></div>
				</div> <% } %>
							</div>
						  </div>

						
					</div>
					<% if(Bat1Name){ %>
						
						<div class="card-body">
							<div class="row">
								<div class="col-md-4 col-sm-12">
									<div class="row">
										<div class="col-2">
											<img id="bat1img" height="50px" width="50px" src="https://m.cricbuzz.com/a/img/v1/192x192/i1/c182026/shams-mulani.jpg" alt="">
										</div>
										<div class="col-10">
											<h6><span id="bat1name"><%= Bat1Name %></span></h6>
											<b><span id="bat1run"><%= Bat1Run %></span></b>
										</div>
									</div>
								</div>
								<div class="col-md-4 col-sm-12">
									<div class="row">
										<div class="col-2">
											<img id="bat2img" height="50px" width="50px" src="https://m.cricbuzz.com/a/img/v1/192x192/i1/c182026/shams-mulani.jpg" alt="">
										</div>
										<div class="col-10">
											<h6><span id="bat2name"><%= Bat2Name %></span></h6>
											<b><span id="bat2run"><%= Bat2Run %></span></b>
										</div>
									</div>
								</div>
								<div class="col-md-4 col-sm-12">
									<div class="row">
										<div class="col-2">
											<img id="bowlerimg" height="50px" width="50px" src="https://m.cricbuzz.com/a/img/v1/192x192/i1/c182026/shams-mulani.jpg" alt="">
										</div>
										<div class="col-10">
											<h6><span id="bowlerName"><%= BowlerName %></span></h6>
											<b><span id="bowlerOW"><%= BowlerOver %>-<%= BowlerWK %></span></b>
										</div>
									</div>
									</div>
								</div>
							</div>
							<div class="bg-light px-4 py-2">
								<div class="row">
									<div class="col-md-4">Last Balls:<span id="lbb"><%= LBB %></span></div>
									<div class="col-md-4">Partnership <span id="ps"><%= PS %></span></div>
									<div class="col-md-4">Last WK:<span id="lw"><%= LW %></span></div>
								</div>
							</div>
						  </div>
					
					
						<% } %>
					  
			 </div>
		  <% } else{ %>  
			<h1>Match Will Start Soon</h1>
		 <% } %>
		<div id="cbox">
			<h4>Chats</h4>
			<div id="myDIV" class="mx-2">
			   <ul class="list-group">
				<%messagelist.forEach(msg => {%>
				   <li class="list-group-item"><b><%= msg.name %></b>:<%= msg.message %></li>
				<%})%>
			   </ul>
			</div>
			<div class="row mb-4">
				<div class="col-8 mb-2">
					<input placeholder="Message" class="form-control" id="usrmsg">
				</div>
				<div class="col-4">
					<button class="btn w-100 btn-primary" onclick="chat()">Send</button>
				</div>
				<div class="col-12">
					<input placeholder="Write Your Name" class="form-control" onchange="nameSet()" id="usrnme">
				</div>
			 </div>
		</div>
		</div>
			</div>
	</div>
	<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>


